@model IEnumerable<ActorModel>

<table class="table" style="table-layout: fixed">
    <tr>
        <th style="color: lemonchiffon ; font-size: large">Actors</th>
        <th style="color: lemonchiffon ; font-size: large">Characters</th>
    </tr>

    @foreach (var actor in Model)
    {
        <tr>
            <td style="color: white ; font-size: medium">@actor.Actor</td>
            <td style="color: white ; font-size: medium">@actor.Character</td>
        </tr>
    }

</table>